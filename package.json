{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.8.0"
  },
  "dependencies": {
    "hexo": "^3.8.0",
    "hexo-algolia": "^1.3.1",
    "hexo-asset-image": "git+https://github.com/CodeFalling/hexo-asset-image.git",
    "hexo-deployer-git": "^0.3.1",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-searchdb": "^1.0.8",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "hexo-symbols-count-time": "^0.6.0",
    "hexo-wordcount": "^6.0.1",
    "save": "^2.4.0"
  },
  "scripts": {
    "start": "hexo clean && hexo server",
    "deploy": "hexo generate --deploy",
    "prepush": "echo \" Begin Auto Push \" ",
    "push": "git status && git add . && read -p \"please input commit info:\" commitInfo && git commit -m \"$commitInfo\" && git pull && git push",
    "postpush": "echo \" End Auto Push \" ",
    "prepage": "echo \" already set post_asset_folder = true \" ",
    "page": "read -p \"please input page title:\" pageName && hexo new page \"$pageName\" ",
    "post": " read -p \"please input post title:\" postName && hexo new post \"$postName\" "
  }
}
